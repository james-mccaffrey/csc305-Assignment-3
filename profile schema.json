{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": {
    "type": "object",
    "properties": {
      "age": {
        "type": "integer",
        "minimum": 0
      },
      "color": {
        "type": "integer",
        "minimum": 0,
        "maximum": 2
      },
      "height": {
        "type": "integer",
        "minimum": 0
      },
      "image": {
        "type": "string",
        "format": "uri"
      },
      "name": {
        "type": "string"
      }
    },
    "required": [ "age", "color", "height", "image", "name" ]
  }
}
